{
  "name": "react-universal-renderer",
  "version": "0.0.12",
  "description": "React Universal Renderer",
  "author": "Anthony Corcutt <acorcutt@gmail.com>",
  "main": "build/index.js",
  "homepage": "https://github.com/acorcutt/react-universal-renderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/acorcutt/react-universal-renderer.git"
  },
  "keywords": [
    "react",
    "universal",
    "renderer",
    "component",
    "react-component"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acorcutt/react-universal-renderer/issues"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "nodemon --watch package.json --watch source --ext js,json,css --exec npm run build",
    "build": "babel source --out-dir build --source-maps --copy-files --presets react,es2015,stage-0"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.10.0",
    "eslint-plugin-react": "^5.1.1",
    "nodemon": "^1.9.1"
  }
}
